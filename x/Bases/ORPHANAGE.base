views:
  - type: table
    name: No Outlinks (+ folder only)
    filters:
      and:
        - file.links.isEmpty()
        - file.ext == "md"
        - '!file.inFolder("Atlas/Sources/Articles (Readwise)")'
        - '!file.inFolder("Atlas/Sources/Books (Readwise)")'
        - '!file.inFolder("Atlas/Sources/Tweets (Readwise)")'
        - file.inFolder("+")
    order:
      - formula.âœ¨
      - file.name
      - created
      - collections
      - up
      - related
      - file.backlinks
    sort:
      - property: created
        direction: DESC
    columnSize:
      note.created: 137
      note.up: 154
      note.related: 146
      note.collections: 199
  - type: table
    name: No Outlinks (Atlas only)
    filters:
      and:
        - file.links.isEmpty()
